{
  "name": "redi-launcher",
  "version": "1.0.0",
  "description": "A custom Minecraft-mod launcher for a school project.",
  "main": "exec.js",
  "scripts": {
    "start": "electron .",
    "dev": "start npm run dev-sass-watcher && electron .",
    "dev-sass": "node-sass sass/style.scss -o ./assets/css",
    "dev-sass-watcher": "npm run dev-sass && node-sass ./sass/style.scss -wo ./assets/css/",
    "postinstall": "install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --win portable",
    "dist-all": "electron-builder -mw"
  },
  "build": {
    "appId": "yourappid",
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "mac": {
      "category": "Games"
    }
  },
  "author": "RediPanda",
  "license": "ISC",
  "dependencies": {
    "cryptr": "^6.0.2",
    "download": "^8.0.0",
    "download-file": "^0.1.5",
    "extract-zip-promise": "^1.0.5",
    "fs-extra": "^9.0.1",
    "gamedig": "^2.0.20",
    "moment": "^2.26.0",
    "node-fetch": "^2.6.7",
    "questions": "0.0.6",
    "randomatic": "^3.1.1",
    "v8-compile-cache": "^2.1.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "bootstrap": "^4.5.0",
    "electron": "^11.5.0",
    "electron-builder": "^22.7.0",
    "electron-reload": "^1.5.0",
    "node-sass": "^4.14.1"
  }
}
